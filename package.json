{
  "name": "redleaf",
  "version": "1.0.0",
  "description": "Model for all authentication of user",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/redleaf-redis/RedLeaf"
  },
  "scripts": {
    "pretest": "npm run lint || true",
    "test": "NODE_ENV=test mocha -u bdd -R spec --require babel-core/register",
    "clean": "rimraf lib && mkdir lib",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "build": "npm run clean && npm run build:module",
    "build:module": "babel -d ./lib ./src -s",
    "postinstall": "npm run build",
    "watch:babel": "babel ./src --watch --out-dir ./lib",
    "commit": "git add . && git-cz"
  },
  "author": "David Alejandro Londo√±o",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "6.16.0",
    "babel-core": "6.17.0",
    "babel-plugin-transform-async-to-generator": "6.16.0",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-es2015-node5": "1.2.0",
    "babel-preset-stage-3": "6.17.0",
    "debug": "2.2.0",
    "ioredis": "2.4.0",
    "koa-jwt": "1.2.0",
    "rimraf": "2.5.4"
  },
  "devDependencies": {
    "arrow-mocha": "^1.0.1",
    "babel-eslint": "7.0.0",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "commitizen": "2.8.6",
    "cz-conventional-changelog": "^1.2.0",
    "dirty-chai": "1.2.2",
    "eslint": "3.7.1",
    "eslint-config-airbnb-base": "8.0.0",
    "eslint-plugin-mocha": "4.6.0",
    "mocha": "3.1.0",
    "sinon": "1.17.5",
    "sinon-as-promised": "4.0.2",
    "slugid": "1.1.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
